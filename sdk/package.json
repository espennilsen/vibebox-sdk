{
  "name": "@vibebox/sdk",
  "version": "1.1.0",
  "description": "Official TypeScript SDK for VibeBox - Programmatic sandbox management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:sdk && npm run build:cli",
    "build:sdk": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:cli": "cd cli && npm run build",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "dev:cli": "cd cli && npm run dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cli": "cd cli && npm test",
    "lint": "eslint src --ext .ts && cd cli && npm run lint",
    "typecheck": "tsc --noEmit && cd cli && npm run typecheck",
    "prepublishOnly": "npm run build",
    "cli:install": "cd cli && npm install"
  },
  "keywords": [
    "vibebox",
    "sandbox",
    "docker",
    "development",
    "sdk",
    "containers",
    "ai",
    "claude-code"
  ],
  "author": "VibeBox Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vibebox-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vibebox-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/vibebox-sdk#readme",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.57.0",
    "msw": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.4",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
